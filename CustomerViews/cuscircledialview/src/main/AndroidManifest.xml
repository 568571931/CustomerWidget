    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.InstrumentActivity" />
        <activity android:name=".activity.ShowPercentActivity" />
        <activity android:name=".activity.DialCircleActivity" />
        <activity android:name=".activity.QKDegreeActivity" />
        <activity android:name=".activity.QKStepActivity" />
        <activity android:name=".activity.Activity1" />
    </application>

</manifest>